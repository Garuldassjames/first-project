 package practice_code;

import java.util.Iterator;

public class loopsstarandcross {
public static void main(String[] args) {
	
	
	for (int i = 1; i <=5; i++) {
		for (int j = 1; j <=i; j++) {
			if (j%2==1) {
				System.out.println("+ ");
			}else {
				System.out.println("* ");
			}
		}	
		System.out.println();
	}
	
	
}
}
