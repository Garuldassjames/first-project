package practice_code;

import java.util.Iterator;

public class loopsstarandcross {
public static void main(String[] args) {
	
	
	for (int i = 1; i <=5; i++) {
		for (int j = 1; j <=i; j++) {
			if (j%2==0) {
				System.out.println("* ");
              if (!((j%2)==0)) {
            	  System.out.println("+ ");
					
				}
			}
		}
		
	}
	
	
}
}
